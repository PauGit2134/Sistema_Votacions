{
  "name": "sistema-votacions-electron",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "electron .",
    "electron:start": "vite build && electron .",
    "electron:dev-start": "set NODE_ENV=development&& electron .",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.7",
    "@mdi/font": "7.4.47",
    "chart.js": "^4.5.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^39.2.4",
    "electron-builder": "^24.13.3",
    "eslint-config-vuetify": "^4.2.0",
    "sass-embedded": "^1.92.1",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.1.5",
    "vite-plugin-vuetify": "^2.1.2"
  },
  "build": {
    "appId": "com.votacions.app",
    "productName": "Sistema Votacions",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "icon": "public/favicon.ico"
    }
  }
}
